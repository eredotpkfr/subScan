{
    "request": {
        "headers": {
            "Authorization": {
                "equalTo": "censys-api-key"
            }
        },
        "method": "GET",
        "queryParameters": {
            "cursor": {
                "absent": true
            }
        },
        "urlPath": "/censys"
    },
    "response": {
        "headers": {
            "content-type": "application/json"
        },
        "jsonBody": {
            "result": {
                "hits": [
                    {
                        "names": [
                            "bar.foo.com",
                            "*.bar.foo.com"
                        ]
                    },
                    {
                        "names": {
                            "foo": "bar"
                        }
                    }
                ],
                "links": {
                    "next": "cursor"
                }
            }
        },
        "status": 200
    }
}
