{
    "request": {
        "method": "GET",
        "queryParameters": {
            "num": {
                "equalTo": 100
            },
            "q": {
                "equalTo": "site:foo.com"
            }
        },
        "urlPath": "/search"
    },
    "response": {
        "body": "<div><cite>bar.foo.com</cite></div>",
        "headers": {
            "content-type": "text/html"
        },
        "status": 200
    }
}
